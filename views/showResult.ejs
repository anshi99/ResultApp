<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <title>Result Managment</title>
  </head>
  <body>
    <nav class="navbar navbar-light"  style="background-color: rgb(41, 180, 133);">
        <!-- Navbar content -->
        <div class="text-center mt-1 ms-2">
            <img src="CBSE-Black.png" height="50px">
        </div>
        
        <div class="mx-auto">
            <h3  style="color:rgb(12, 11, 11);"><b>Result Managment System</b></h3>
        </div>
        <div class="me-2">
            <button type="button" class="btn btn-danger btn-sm">Logout</button>
        </div>
        
    </nav>

    <main>
        <div style="margin-top: 30px; margin-left: 10px;">
            <a href="/student/login" ><button type="button" class="btn btn-warning">Go Back</button></a>
        </div>


        <div class="container" style="margin-top: 5%;">
            <div class="row">
                <div class="col-12 text-center">
                  <h5><b style="color: darkred;">RESULT</b></h5>
                </div>
              </div>
        </div>
        <div class="container" style="margin-top: 10px;">
            <div class="row">

                <div class="col-6 text-end">
                    <label style="font-weight: 100;"><b>ROLL NUMBER </b></label>
                </div>
                <div class="col-6 text-start">
                    <label id="rollnovalue" style="color: green;"><B><%=studentData.rollnumber%></B></label>
                </div>

              </div>

              <div class="row mt-1">

                <div class="col-6 text-end">
                    <label style="font-weight: 100;"><b>SUDENT NAME </b></label>
                </div>
                <div class="col-6 text-start">
                    <label id="fullname" style="color: green;"><B><%=studentData.fullname%></B></label>
                </div>

              </div>

              <div class="row mt-1">

                <div class="col-6 text-end">
                    <label style="font-weight: 100;"><b>DATE OF BIRTH </b></label>
                </div>
                <div class="col-6 text-start">
                    <label id="dob" style="color: green;"><B><%=studentData.dob%></B></label>
                </div>

              </div>

              <div class="row mt-1">

                <div class="col-6 text-end">
                    <label ><b>SCORE </b></label>
                </div>
                <div class="col-6 text-start">
                    <label id="score" style="color: green;"><B><%=studentData.score%></B></label>
                </div>

              </div>
        </div>
    </main>

   

  </body>
</html>